.main-ui-panel {
    height: 100%;
    min-height: 100%;
    flex-direction: column-reverse;
}

.button-container {
    flex-direction: column;
    justify-content: flex-end;
    align-items: flex-end;
    width: 100%;
    height: 100%;
    display: flex;
}

.button {
    background-color: rgb(102, 167, 255);
    padding: 10px;
    margin: 5px;
}

.main-list-view {
    align-items: center;
}

.hidden {
    display: none;
}

.main-scroll-view{
    align-items: center;
}